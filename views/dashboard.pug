extends public
block append style
  each css in styleList
    link(rel='stylesheet', href=css)

block append main
  h1 DashBoard
  each post in posts
    p
      a(href=`/post/${post.year}/${post.month}/${post.link}`)=post.title
  div#root

block append script
  script.
    var __ENV__ = '#{ENV}'
  each js in scriptList
    script(src=js)
